@{
    ViewData["Title"] = "Hem";
}

<div class="text-center">
    <h1 class="display-5 mb-3">Välkommen till Othello</h1>
    <p class="lead">Skapa nya användare eller visa befintliga direkt via länkarna nedan.</p>
    <div class="mt-4">
        <a class="btn btn-primary me-2" asp-controller="User" asp-action="RegisterUser">Registrera användare</a>
        <a class="btn btn-outline-secondary" asp-controller="User" asp-action="SelectUser">Visa användare</a>
    </div>
</div>
